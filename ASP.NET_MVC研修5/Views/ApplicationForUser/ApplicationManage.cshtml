            $("#lblエラー").html("");
            var checked = false;
            var havedata = $('input[id^="chk選択"]').length;
            $('input[id^="chk選択"]:checked').each(function (index, elem) {
                checked = true;
                return false;
            });

            if (checked) {
                if (confirm("選択されているユーザーアカウントを有効にしますが、よろしでしょうか。")) {
                    $("#Form1").attr("Method", "Post");
                    $("#Form1").attr("action", "EnableUser");
